{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KeyVault": {
    "Enabled": false,
    "VaultUri": "https://<VAULT_NAME>.vault.azure.net/"
  },
  "RecipeApp": {
    "ImageStoreType": "FileSystem",
    "ImageContainerId": "AppBlob",
    "EnableAuthentication": true,
    "EnableSeeding": false
  },
  "Database": {
    "SqlServer": {
      "CommandTimeoutSeconds": 180,
      "MaxRetryCount": 10,
      "MaxRetryDelaySeconds": 30
    }
  },
  "Authentication": {
    "EntraId": {
      "Enabled": false,
      "TenantId": "<TENANT_ID>",
      "Authority": "https://login.microsoftonline.com/<TENANT_ID>",
      "ClientId": "<CLIENT_ID>",
      "ClientSecret": "",
      "CallbackPath": "/signin-oidc",
      "SignedOutCallbackPath": "/signout-callback-oidc"
    }
  },
  "BlobStorage": {
    "Containers": [
      {
        "StoreType": "Filesystem",
        "ContainerId": "AppBlob",
        "Parameters": {
          "RootPath": "/tmp/AppBlob",
          "ReadOnly": "false"
        }
      },
      {
        "StoreType": "Azure",
        "ContainerId": "AppBlob",
        "Parameters": {
          "ServiceUri": "https://<STORAGEACCT>.blob.core.windows.net",
          "ContainerName": "<CONTAINER_NAME>",
          "CreateIfNotExists": "false",
          "ReadOnly": "false"
        }
      }
    ]
  }
}
