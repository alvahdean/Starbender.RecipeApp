
@inherits ComponentBase

<h6>Recipes</h6>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in _recipes)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td><RecipeIngredientList Recipe="item" /></td>

            </tr>
        }
    </tbody>
</table>


